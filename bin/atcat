#!/usr/bin/env ruby
require 'rubygems'
require 'abtab'

uri = ARGV.shift or raise "You must speicfy a URI to cat"

inp = Abtab.read_handle uri

outp = nil
if ARGV.empty?
  outp = Abtab.write_handle 'tab:///dev/stdout'
else
  outp = Abtab.write_handle ARGV.shift
end

outp.set_columns inp.columns

outp.import inp

